/**
 * @param {number[]} nums
 * @return {number}
 */
var singleNumber = function(nums) {
    let tmpMap = new Map();
    for(let i = 0; i < nums.length; i++){
        let strNum = nums[i].toString();
        if(tmpMap.has(strNum)){
            tmpMap.delete(strNum);
        } else{
            tmpMap.set(strNum, true);
        }
    }
    return tmpMap.keys().next().value;
};
