/**
 * @param {number[]} nums
 * @param {number} k
 * @return {boolean}
 */
var containsNearbyDuplicate = function(nums, k) {
    let tmpMap = new Map();
    for(let i = 0; i < nums.length; i++){
        if(tmpMap.has( nums[i].toString() ) &&
           Math.abs( tmpMap.get(nums[i].toString()) - i) <= k){
            return true;
        } else{
            tmpMap.set(nums[i].toString(), i);
        }
    }
    return false;
};
