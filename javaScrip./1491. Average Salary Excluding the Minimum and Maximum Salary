/**
 * @param {number[]} salary
 * @return {number}
 */
var average = function(salary) {
    salary.sort(function(a, b){return a-b});
    let counter = 1,
        aggregatedSalary = 0;
    while(counter < salary.length - 1){
        aggregatedSalary += salary[counter++];
    }
    if(counter > 1) return aggregatedSalary/(counter - 1);
    return 0;
};
